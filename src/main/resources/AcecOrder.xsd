  <!-- SIPS Payment (ATOS) -->
  <element name="sipsPayment" type="aceco:sipsPayment">
    <annotation>
      <documentation>ATOS SIPS Payment</documentation>
    </annotation>
  </element>
  <complexType name="sipsPayment">
    <complexContent mixed="false">
      <extension base="aceco:abstractPayment">
        <sequence>
          <element name="authorizationId" type="string">
            <annotation>
              <documentation>Payment authorization code</documentation>
            </annotation>
          </element>
          <element name="bankResponseCode" type="integer">
            <annotation>
              <documentation>Bank authorization response code</documentation>
            </annotation>
          </element>
          <element name="maskedCreditCardNumber" type="string">
            <annotation>
              <documentation>Masked Credit Card Number - XXXX**********YY</documentation>
            </annotation>
          </element>
          <element name="cvvFlag" type="integer">
            <annotation>
              <documentation>Field returned in the response in the case of a payment with a card having a check number. Refer to APPENDIX B.</documentation>
            </annotation>
          </element>
          <element name="cvvResponseCode" type="string">
            <annotation>
              <documentation>Field returned in the response in the case of a payment with a card having a check number. Refer to APPENDIX B.</documentation>
            </annotation>
          </element>
          <element name="paymentCertificate" type="string">
            <annotation>
              <documentation>Field returned in the response in case an authorisation request is accepted. The value in this field is calculated from the elements of the transaction and certifies that the authorisation request has been processed by SIPS.</documentation>
            </annotation>
          </element>
          <element name="paymentDateTime" type="dateTime">
            <annotation>
              <documentation>Data/hora do pagamento</documentation>
            </annotation>
          </element>
          <element name="billingType" type="string">
            <annotation>
              <documentation>Tipo do pagamento - CB, VISA, MASTERCARD, PASSCARD, PASSMASTERCARD, AMEX</documentation>
            </annotation>
          </element>
          <element name="responseCode" type="string">
            <annotation>
              <documentation>Field returned in the response and containing the code of the SIPS Server’s response. Refer to APPENDIX G.</documentation>
            </annotation>
          </element>
          <element name="transactionCondition" type="string">
            <annotation>
              <documentation>Contains the payment authentication result. 3D_SUCCESS, 3D_FAILURE, 3D_ERROR, 3D_NOTENROLLED, 3D_ATTEMPT, SSL. Refer to APPENDIX J</documentation>
            </annotation>
          </element>
          <element name="currencyCode" type="string">
            <annotation>
              <documentation>Codigo da moeda de pagamento baseado no ISO-4217 - Alpha 3. Ex: EUR, USD, BRL</documentation>
            </annotation>
          </element>
          <element name="transmitionDateTime" type="dateTime">
            <annotation>
              <documentation>Data/hora da transmissao</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
