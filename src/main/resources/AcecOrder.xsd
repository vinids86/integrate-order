<?xml version="1.0" encoding="utf-8"?>

<!-- @author: Wesley Akio Imamura - Accurate Software-->

<schema xmlns:aceco="http://www.accurate.com/acec/order"
  attributeFormDefault="unqualified"
  elementFormDefault="qualified"
  targetNamespace="http://www.accurate.com/acec/order"
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:invoice="http://www.accurate.com/acec/InvoiceInfo"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
  jaxb:extensionBindingPrefixes="xjc"
  jaxb:version="1.0"  
  >
  <import schemaLocation="AcecInvoiceInfo.xsd"
         namespace="http://www.accurate.com/acec/InvoiceInfo"/>
    

  <!-- IMPORTANTE: Quando for gerar as entidades JAXB desse XSD, 
  deve-se gerar tambem as entidades do AcecInvoiceInfo.xsd no mesmo 
  pacote JAR.
  Para isso selecione ambos os arquivos quando executar o wizard
  de geracao -->
  
  <!-- FORCE ALL CLASSES IMPLEMENTS SERIALIZABLE -->
  <annotation>
    <appinfo>
      <jaxb:globalBindings generateIsSetMethod="true">
        <xjc:serializable uid="1"/>
      </jaxb:globalBindings>
    </appinfo>
  </annotation>
  
  <element name="order" type="aceco:order" />
  <element name="deliveries" type="aceco:deliveries" />
  <complexType name="order">
    <annotation>
      <documentation>Pedido</documentation>    
    </annotation>

    <sequence>
      <element name="orderId" type="string">
        <annotation>
          <documentation>Id do pedido</documentation>
        </annotation>
      </element>
      
      <element name="customerId" type="string">
        <annotation>
          <documentation>Id do cliente</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="partnerId" type="string">
        <annotation>
          <documentation>Id do parceiro</documentation>
        </annotation>
      </element>    
      
      <element minOccurs="1" name="totalAmount" type="decimal">
        <annotation>
          <documentation>Valor total do pedido, já com descontos, frete, etc</documentation>
        </annotation>
      </element>    
      
      <element name="totalDiscountAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos descontos aplicados ao Pedido</documentation>
        </annotation>
      </element>
        
      <element name="totalLoyaltyCreditEarningAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos créditos de loyalty do pedido</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="partnerOrderId" type="string">
        <annotation>
          <documentation>Id do pedido no parceiro</documentation>
        </annotation>
      </element>
      
      <element name="purchaseDate" type="dateTime">
        <annotation>
          <documentation>Data e hora da geração do pedido</documentation>
        </annotation>
      </element>    
        
      <element name="visitorIp" type="string">
        <annotation>
          <documentation>IP do cliente</documentation>
        </annotation>
      </element>   
         
      <element minOccurs="0" name="visitorId" type="string">
        <annotation>
          <documentation>Id para rastreamento de cliente anônimo (não autenticado)</documentation>
        </annotation>
      </element>     
      
      <element minOccurs="0" name="salesOperatorId" type="string">
        <annotation>
          <documentation>Id do operador de venda</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="salesOperatorName" type="string">
      	<annotation>
      		<documentation>Nome do operador de venda</documentation>
      	</annotation>
      </element>
      
      <element minOccurs="0" name="sessionCreationDate" type="dateTime">
        <annotation>
          <documentation>Data/Hora da criação da sessão</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="applicationVersion" type="string">
        <annotation>
          <documentation>Versão da aplicação que gerou o pedido</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="listId" type="string">
        <annotation>
          <documentation>Identificador da lista que originou o pedido (lista de casamento, desejos, etc)</documentation>
        </annotation>
      </element>
      
      <element name="saleChannel" type="string">
        <annotation>
          <documentation>Canal de venda</documentation>
        </annotation>
      </element>
      
      <element name="shopId" type="string">
        <annotation>
          <documentation>Identificador da loja fisica</documentation>
        </annotation>
      </element>
      
      <element name="leader" type="string">
        <annotation>
          <documentation>Lider da loja fisica</documentation>
        </annotation>
      </element>
      
      <element name="orderType" >
        <annotation>
          <documentation>Tipo do pedido (normal, presenteado, outros)</documentation>
        </annotation>
        <simpleType>
              <restriction base="string">
                <enumeration value="NORMAL" />
                <enumeration value="PRESENT" />
                <enumeration value="OTHER" />
              </restriction>
            </simpleType>
      </element>
      
      <element minOccurs="0" name="brandId" type="string">
        <annotation>
          <documentation>Id da marca</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="storeId" type="string">
        <annotation>
          <documentation>Id da loja</documentation>
        </annotation>
      </element>     
      <element minOccurs="0" name="orderMasterId" type="string">
 		  <annotation>
          	<documentation>Id do pedido mestre</documentation>
		  </annotation>
	  </element>        
      <element name="deliveries" type="aceco:deliveries">
        <annotation>
          <documentation>Lista de entregas</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="orderPropertyList">
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="orderProperty" type="aceco:orderProperty" />
          </sequence>
        </complexType>
      </element>
      
      <element name="customer" type="aceco:customer">
        <annotation>
          <documentation>Endereço de cobrança, endereço principal do cliente</documentation>
        </annotation>
      </element>     
      
      <element name="billingAddress" type="aceco:address">
        <annotation>
          <documentation>Endereço de cobrança, endereço principal do cliente</documentation>
        </annotation>
      </element>     
      
      <element name="paymentList"> 
        
        <annotation>
          <documentation>Informações de meios de pagamento utilizados nesse pedido</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="payment" type="aceco:payment">
              <annotation>
                <documentation>Pagamento específico</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      
      <element minOccurs="0" name="freightChargedAmount" type="decimal">
        <annotation>
          <documentation>Valor final do frete</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="freightActualAmount" type="decimal">
        <annotation>
          <documentation>Valor nominal do frete (sem descontos e abatimentos, com margem comercial)</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="freightCommercialAmount" type="decimal">
        <annotation>
          <documentation>Valor somente da margem comercial</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="countDistinctSku" type="integer">
        <annotation>
          <documentation>Número de sku distintos no fechamento de pedido</documentation>
        </annotation>
      </element>
   	  <element minOccurs="0" name="orderOwnerTp" type="integer">
		<annotation>
			<documentation>Owner of the Order - 1 (Default - Order from Ckout) - 2 (External - Order integrated by WS)</documentation>
		</annotation>
      </element>
      <element name="shopInfo" type="aceco:shopInfo" minOccurs="0">
        <annotation>
          <documentation>Shop info</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="orderLine">

     <annotation>
      <documentation>Linha do pedido</documentation>    
    </annotation>
    
    <sequence>
    
      <element name="orderLineId" type="string">
        <annotation>
          <documentation>Id da linha do pedido</documentation>
        </annotation>
      </element>
      
      <element name="sku" type="string">
        <annotation>
          <documentation>SKU do produto</documentation>
        </annotation>
      </element>
      
      <element name="skuIdOrigin" type="string">
        <annotation>
          <documentation>SKU Origem do produto</documentation>
        </annotation>
      </element>

      <element name="skuType" type="string">
        <annotation>
          <documentation>Tipo do produto</documentation>
        </annotation>
      </element>
      
      <element name="quantity" type="int">
        <annotation>
          <documentation>Quantidade de itens nessa linha</documentation>
        </annotation>
      </element>
      
      <element name="measureUnit" type="string">
        <annotation>
          <documentation>Unidade de medida do produto (PIECE, KG, L)</documentation>
        </annotation>
      </element>
      
      <element name="catalogListPrice" type="decimal">
        <annotation>
          <documentation>Valor unitário do produto no catálogo</documentation>
        </annotation>
      </element>
      
      <element name="listPrice" type="decimal">
        <annotation>
          <documentation>Valor unitário de catálogo do produto</documentation>
        </annotation>
      </element>
      
      <element name="salePrice" type="decimal">
        <annotation>
          <documentation>Valor unitário de venda do produto</documentation>
        </annotation>
      </element>
      <element name="replenishmentInfo" type="aceco:replenishmentInfo" minOccurs="0">
      	<annotation>
      		<documentation>Lista de informações do stock</documentation>
      	</annotation>
      </element>
      
      <element name="unconditionalDiscountAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos descontos incondicionais aplicados à linha</documentation>
        </annotation>
      </element>
      
      <element name="conditionalDiscountAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos descontos condicionais aplicados à linha</documentation>
        </annotation>
      </element>
      
      <element name="roundingDiscountAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos arredondamentos de descontos aplicados à linha</documentation>
        </annotation>
      </element>
      
      <element name="loyaltyCreditEarningAmount" type="decimal">
        <annotation>
          <documentation>Valor total de créditos de loyalty da linha</documentation>
        </annotation>
      </element>

      <element name="roundingLoyaltyCreditEarningAmount" type="decimal">
        <annotation>
          <documentation>Valor total dos arredondamentos de créditos de loyalty da linha</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="promotionList">
        <annotation>
          <documentation>Lista de promoções aplicadas para essa linha</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="promotion" type="aceco:promotion">
              <annotation>
                <documentation>Lista de promoções aplicacadas</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      <element minOccurs="0" name="loyaltyCreditList">
          <annotation>
	          <documentation>Lista de créditos do programa de fidelidade para essa linha</documentation>
          </annotation>
		  <complexType>
          	<sequence>
            	<element maxOccurs="unbounded" name="loyaltyCredit" type="aceco:loyaltyCredit">
                	<annotation>
                    	<documentation>Lista de Créditos do programa de fidelidade aplicacadas</documentation>
				    </annotation>
		        </element>
            </sequence>
          </complexType>
      </element>
      <element name="freight" type="aceco:freight">
        <annotation>
          <documentation>Informações de frete</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="leadTime" type="int">
        <annotation>
          <documentation>Prazo que o fornecedor leva para entregar a mercadoria ao CD</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="giftCard" type="aceco:giftCard">
        <annotation>
          <documentation>Dados do cartão de presente</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="giftWrap" type="aceco:giftWrap">
        <annotation>
          <documentation>Dados de embalagem para presente</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="kitSkuId" type="string">
        <annotation>
          <documentation>SKU do kit, caso essa linha seja parte de um</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="kitSkuName" type="string">
        <annotation>
          <documentation>Nome do kit, caso essa linha seja parte de um</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="kitQuantity" type="int">
        <annotation>
          <documentation>Quantidade de kits, caso essa linha seja parte de um</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="reference">
        <annotation>
            <documentation>Lista de referência cruzada: sku x garantia, garantia x sku, sku x brinde, brinde x sku</documentation>
		</annotation>
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="skuId" type="string">
              <annotation>
                <documentation>Sku de referência</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
      
      <element minOccurs="0" name="catalogId" type="string">
        <annotation>
          <documentation>Id do catálogo que originou a adição do produto na sacola</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="catalogReference" type="string">
        <annotation>
          <documentation>Referência a posição do produto no catálogo</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="orderLinePropertyList">
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="orderLineProperty" type="aceco:orderLineProperty" />
          </sequence>
        </complexType>
      </element>
      
      <element name="inventoryType" type="string">
        <annotation>
          <documentation>Tipo da reserva</documentation>
        </annotation>
      </element>
      
       <element name="service" type="boolean">
        <annotation>
          <documentation>Se é um serviço ou não</documentation>
        </annotation>
      </element>
      
      <element name="skuName" type="string">
        <annotation>
          <documentation>Nome do sku</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="productBrandName" type="string">
		<annotation>
            <documentation>Nome da marca do produto</documentation>
		</annotation>
	  </element>      
      <element minOccurs="0" name="itemTotalFinancialExpense" type="decimal">
        <annotation>
          <documentation>Valor total das despesas financeira do item (juros). Valor não setado pelo Checkout</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="itemLevelList">
        <annotation>
          <documentation>Lista de Categoria principal(main) do Site(vendas) para esse Item</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="itemLevel" type="aceco:level">
              <annotation>
                <documentation>Lista de Categoria principal(main) do Site(vendas) para esse Item</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>

      <element minOccurs="0" name="itemLevelErpList">
        <annotation>
          <documentation>Lista de Categoria principal(main) do Site(vendas) para esse Item</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="itemLevel" type="aceco:level">
              <annotation>
                <documentation>Lista de Categoria principal(main) do Site(vendas) para esse Item</documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <element name="replenishment" type="aceco:replenishmentInfo"/>
  <complexType name="replenishmentInfo">
    <sequence minOccurs="1">
      <element name="quantity" type="string"/>
      <element name="stockStartDate" type="dateTime"/>
    </sequence>
  </complexType>
  <element name="tracking" type="aceco:tracking"/>
  <complexType name="tracking">
     <annotation>
      <documentation>Tracking</documentation>    
    </annotation>
    <sequence>
      <element minOccurs="0"  name="trackingId" type="string">
        <annotation>
          <documentation>Id do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingCode" type="string">
        <annotation>
          <documentation>Codigo do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingDate" type="dateTime">
        <annotation>
          <documentation>Data e hora do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingStep" type="string">
        <annotation>
          <documentation>Passo do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingTitle" type="string">
        <annotation>
          <documentation>Título do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingMessage" type="string">
        <annotation>
          <documentation>Mensagem do tracking</documentation>
        </annotation>
      </element>
      <element name="trackingStepStatus" type="string">
        <annotation>
          <documentation>Status do Passo do tracking</documentation>
        </annotation>
      </element>
	  <element minOccurs="0" name="adjustedDeliveryDt" type="dateTime">
      	<annotation>
            <documentation>Data atualizada de entrega</documentation>
		</annotation>
	  </element>
      <element minOccurs="0" name="carrierURL" type="string">
        <annotation>
          <documentation>URL para consulta do pedido na transportadora</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="invoiceURL" type="string">
        <annotation>
          <documentation>URL para recuperação da nota fiscal</documentation>
        </annotation>
      </element>
      <element minOccurs="0" ref="invoice:invoiceInfo">
		<annotation>
            <documentation>Informações referentes a nota fiscal.</documentation>
		</annotation>
	  </element>
      <element minOccurs="0" name="trackingPropertyList">
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="trackingProperty" type="aceco:trackingProperty" />
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <element name="skuDeliveryTracking" type="aceco:skuDeliveryTracking"/>
  <complexType name="skuDeliveryTracking">
     <annotation>
      <documentation>Sku Tracking for a given order</documentation>    
    </annotation>
    <sequence>
      <element minOccurs="1"  name="skuId" type="string">
        <annotation>
          <documentation>SKU ID</documentation>
        </annotation>
      </element>
      <element name="skuName" type="string">
        <annotation>
          <documentation>Nome do SKU</documentation>
        </annotation>
      </element>
      <element name="preparedQt" type="long">
        <annotation>
          <documentation>Quantidade preparada e pronta para despacho/retirada</documentation>
        </annotation>
      </element>
      <element name="unitPrice" type="decimal" minOccurs="0">
        <annotation>
          <documentation>Valor unitário</documentation>
        </annotation>
      </element>
      <element name="totalPrice" type="decimal" minOccurs="0">
        <annotation>
          <documentation>Valor total</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="giftWrap">
     
    <annotation>
      <documentation>Embalagem de Presente</documentation>    
    </annotation>    
    
    <sequence>
      <element name="from" type="string">
        <annotation>
          <documentation>Nome &quot;De:&quot; na embalagem de presente</documentation>
        </annotation>
      </element>
      <element name="to" type="string">
        <annotation>
          <documentation>Nome &quot;Para:&quot; na embalagem de presente</documentation>
        </annotation>
      </element>
      <element name="giftWrapType" type="string">
        <annotation>
          <documentation>tipo de embalagem</documentation>
        </annotation>
      </element>
      <element name="giftWrapPrice" type="decimal">
        <annotation>
          <documentation>Valor de venda da embalagem. (Para a integração valor da despesa do item)</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="giftCard">
    
    <annotation>
      <documentation>Cartão de presente</documentation>    
    </annotation>
    
    <sequence>
      <element name="from" type="string">
        <annotation>
          <documentation>Nome &quot;De:&quot; no cartão</documentation>
        </annotation>
      </element>
      <element name="to" type="string">
        <annotation>
          <documentation>Nome &quot;Para:&quot; no cartão</documentation>
        </annotation>
      </element>
      <element name="message" type="string">
        <annotation>
          <documentation>Mensagem no cartão</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <element name="abstractPayment" type="aceco:abstractPayment">
    <annotation>
      <documentation>Informações de pagamento geral</documentation>
    </annotation>
  </element>
  <complexType name="abstractPayment">
    <annotation>
      <documentation>Dados de pagamento genéricos</documentation>    
    </annotation>
    
    <sequence>
      <element name="paymentId" type="string">
        <annotation>
          <documentation>Id do pagamento sequencial.</documentation>
        </annotation>
      </element>

      <element name="transactionId" type="string">
        <annotation>
          <documentation>Transaction Id</documentation>
        </annotation>
      </element>

      <element name="paymentType" type="string">
        <annotation>
          <documentation>Tipos de pagamento (CREDIT_CARD, BILL, VOUCHER, FINANCE, etc )</documentation>
        </annotation>
      </element>

    <element name="paymentKey" type="string">
      <annotation>
        <documentation>ID de Pagamentos, Ex VISA, DEBT</documentation>
      </annotation>
    </element>

    <element name="documentNumber" type="string">
        <annotation>
          <documentation>Número de referência(código voucher, número financiamento)</documentation>
        </annotation>
    </element>
      
      <element name="value" type="decimal">
        <annotation>
          <documentation>Valor do pagamento</documentation>
        </annotation>
      </element>
      
      <element name="interestRate" type="decimal">
        <annotation>
          <documentation>Taxa de juros</documentation>
        </annotation>
      </element>
      
      <element name="interestAmount" type="decimal">
        <annotation>
          <documentation>Valor absoluto dos juros</documentation>
        </annotation>
      </element>
      
      <element name="dueDate" type="dateTime">
        <annotation>
          <documentation>Data de vencimento</documentation>
        </annotation>
      </element>

      <element name="status" type="string">
        <annotation>
          <documentation>Estado do pagamento</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="bankCode" type="integer">
        <annotation>
          <documentation>Código do banco</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="agencyCode" type="integer">
        <annotation>
          <documentation>Código da agência</documentation>
        </annotation>
      </element>

      <element minOccurs="0" name="accountNumber" type="integer">
        <annotation>
          <documentation>Código da conta corrente</documentation>
        </annotation>
      </element>
      
      <element minOccurs="0" name="paymentPropertyList">
        <complexType>
          <sequence>
            <element maxOccurs="unbounded" name="paymentProperty" type="aceco:paymentProperty" />
          </sequence>
        </complexType>
      </element>
      
    </sequence>
  </complexType>
  <!-- Fim Abstract Payment -->
  
  <complexType name="payment">
    <choice>
      <element ref="aceco:abstractPayment" />
      <element ref="aceco:creditCardPayment" />
      <element ref="aceco:cieloPayment" />
      <element ref="aceco:sipsPayment" />
    </choice>
  </complexType>
  
  <!-- Pagamento com cartao de Credito -->
  <element name="creditCardPayment" type="aceco:creditCardPayment">
    <annotation>
      <documentation>Informações de pagamento com cartão de crédito</documentation>
    </annotation>
  </element>
  <complexType name="creditCardPayment">
    <complexContent mixed="false">
      <extension base="aceco:abstractPayment">
        <sequence>
          <element name="cardHoldersName" type="string">
            <annotation>
              <documentation>Nome exatamente como escrito no cartão</documentation>
            </annotation>
          </element>
          <element name="brand" type="string">
            <annotation>
              <documentation>Bandeira</documentation>
            </annotation>
          </element>
          <element name="numberOfInstallments" type="integer">
            <annotation>
              <documentation>Número de parcelas</documentation>
            </annotation>
          </element>
          <element name="encriptedCreditCardNumber" type="string">
            <annotation>
